# Auth Service - Development Environment

# Server settings
NODE_ENV=development
PORT=8001

# Database settings
DB_HOST=postgres
DB_PORT=5432
DB_USER=admin
DB_PASSWORD=password
DB_NAME=erp_auth

# JWT settings
JWT_SECRET=dev_jwt_secret_key_replace_in_production
JWT_EXPIRATION=24h

# Password hashing
BCRYPT_SALT_ROUNDS=10

# Logging
LOG_LEVEL=debug

# CORS settings
CORS_ORIGIN=*
